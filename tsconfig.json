{
  "$schema": "https://json.schemastore.org/tsconfig",
  "include": [
    "index.js",
    "radix.js"
  ],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "target": "es2022",
    "lib": [
      "es2022"
    ],
    "moduleResolution": "Node",
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "types",
    "declarationMap": true
  }
}
